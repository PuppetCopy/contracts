{
  "name": "@puppet/contracts",
  "license": "BSL-1.1",
  "version": "0.1.0",
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "test": "forge test --match-contract TradingTest -vvv",
    "test:target": "bun run test --match-test AllocationTest",
    "test:fork": "forge test --match-contract TradingForkTest -vvvv --fork-url=arbitrum",
    "build": "forge build",
    "build:prod": "FOUNDRY_PROFILE=prod forge build",
    "generate": "bun run script/generate.ts",
    "build:ts": "tsc -b",
    "docgen": "forge doc --out '.docgen' --build",
    "lint": "forge fmt --check",
    "lint:fmt": "forge fmt"
  },
  "files": [
    "./src",
    "./deployments.toml",
    "./dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js"
    },
    "./abi/*": {
      "types": "./dist/types/abi/*.d.ts",
      "import": "./dist/esm/abi/*.js"
    },
    "./events": {
      "types": "./dist/types/events.d.ts",
      "import": "./dist/esm/events.js"
    },
    "./gmx": {
      "types": "./dist/types/gmx/index.d.ts",
      "import": "./dist/esm/gmx/index.js"
    },
    "./gmx/*": {
      "types": "./dist/types/gmx/*.d.ts",
      "import": "./dist/esm/gmx/*.js"
    },
    "./deployments": "./deployments.json"
  },
  "devDependencies": {
    "@types/bun": "catalog:",
    "dotenv": "catalog:",
    "smol-toml": "^1.6.0",
    "typescript": "catalog:",
    "viem": "catalog:"
  }
}
